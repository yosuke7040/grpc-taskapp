{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "gen": "buf generate ../backend/rpc",
    "gen2": "cd .. && buf generate --template frontend/buf.gen.yaml --path backend/rpc",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint-fix": "pnpm lint --fix",
    "format": "prettier \"**/*\" --ignore-unknown",
    "format-fix": "pnpm format --write",
    "preview": "vite preview"
  },
  "dependencies": {
    "@bufbuild/connect": "^0.13.0",
    "@bufbuild/connect-query": "^0.4.1",
    "@bufbuild/connect-web": "^0.13.0",
    "@bufbuild/protobuf": "^1.10.0",
    "@connectrpc/connect": "^1.4.0",
    "@connectrpc/connect-migrate": "^1.4.0",
    "@connectrpc/connect-web": "^1.4.0",
    "@emotion/react": "^11.11.4",
    "@mantine/core": "^7.11.0",
    "@mantine/form": "^7.11.0",
    "@mantine/hooks": "^7.11.0",
    "@tanstack/react-query": "^5.49.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.34.0",
    "@bufbuild/protoc-gen-connect-query": "^0.4.1",
    "@bufbuild/protoc-gen-es": "^1.10.0",
    "@connectrpc/protoc-gen-connect-es": "^1.4.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "prettier": "^3.3.2",
    "typescript": "^5.2.2",
    "vite": "^5.3.1"
  }
